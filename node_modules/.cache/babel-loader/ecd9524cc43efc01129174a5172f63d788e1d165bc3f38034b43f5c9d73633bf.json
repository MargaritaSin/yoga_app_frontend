{"ast":null,"code":"import { ref, onMounted } from 'vue';\nexport default {\n  setup() {\n    const events = ref([]);\n    const selectedDate = ref();\n\n    // Имитация получения данных о занятиях (можно заменить на запрос к API)\n    function fetchEvents(date) {\n      // здесь логика загрузки данных для конкретной даты\n      events.value = [{\n        id: 1,\n        title: 'Йога для начинающих',\n        time: '10:00',\n        date: date.toLocaleDateString(),\n        instructor: 'Марина',\n        spots: 5\n      }\n      // добавление других событий...\n      ];\n    }\n\n    // Загрузка событий при монтировании компонента\n    onMounted(() => {\n      fetchEvents(selectedDate.value);\n    });\n    function changeDay(direction) {\n      const newDate = new Date(selectedDate.value);\n      newDate.setDate(newDate.getDate() + direction);\n      selectedDate.value = newDate;\n      fetchEvents(selectedDate.value);\n    }\n    return {\n      events,\n      selectedDate,\n      fetchEvents,\n      changeDay\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","setup","events","selectedDate","fetchEvents","date","value","id","title","time","toLocaleDateString","instructor","spots","changeDay","direction","newDate","Date","setDate","getDate"],"sources":["E:\\yoga_kurs\\src\\components\\schedule_element.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"changeDay(-1)\">Предыдущий день</button>\r\n    <span>{{ selectedDate.value.toLocaleDateString() }}</span>\r\n    <button @click=\"changeDay(1)\">Следующий день</button>\r\n\r\n    <div v-for=\"event in events\" :key=\"event.id\" class=\"event\">\r\n      <h2>{{ event.title }}</h2>\r\n      <p>{{ event.time }} - {{ event.date }}</p>\r\n      <p>Инструктор: {{ event.instructor }}</p>\r\n      <p>Еще {{ event.spots }} мест(а)</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n  <script>\r\n    import { ref, onMounted } from 'vue';\r\n\r\n    export default {\r\n      setup() {\r\n        const events = ref([]);\r\n        const selectedDate = ref();\r\n\r\n        // Имитация получения данных о занятиях (можно заменить на запрос к API)\r\n        function fetchEvents(date) {\r\n          // здесь логика загрузки данных для конкретной даты\r\n          events.value = [\r\n            { id: 1, title: 'Йога для начинающих', time: '10:00', date: date.toLocaleDateString(), instructor: 'Марина', spots: 5 },\r\n            // добавление других событий...\r\n          ];\r\n        }\r\n        \r\n        // Загрузка событий при монтировании компонента\r\n        onMounted(() => {\r\n          fetchEvents(selectedDate.value);\r\n        });\r\n\r\n        function changeDay(direction) {\r\n          const newDate = new Date(selectedDate.value);\r\n          newDate.setDate(newDate.getDate() + direction);\r\n          selectedDate.value = newDate;\r\n          fetchEvents(selectedDate.value);\r\n        }\r\n\r\n        return { events, selectedDate, fetchEvents, changeDay };\r\n      }\r\n}\r\n  </script>\r\n\r\n  <style>\r\n  .schedule {\r\n    background: #333;\r\n    color: #fff;\r\n    padding: 20px;\r\n  }\r\n  .event {\r\n    background: #444;\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n    border-radius: 8px;\r\n  }\r\n\r\n  h1, h2 {\r\n    color: #acd0f0;\r\n    font-weight: 700;\r\n    font-size: 30px;\r\n  }\r\n  </style>\r\n  \r\n"],"mappings":"AAgBI,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AAEpC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIH,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMI,YAAW,GAAIJ,GAAG,CAAC,CAAC;;IAE1B;IACA,SAASK,WAAWA,CAACC,IAAI,EAAE;MACzB;MACAH,MAAM,CAACI,KAAI,GAAI,CACb;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,qBAAqB;QAAEC,IAAI,EAAE,OAAO;QAAEJ,IAAI,EAAEA,IAAI,CAACK,kBAAkB,CAAC,CAAC;QAAEC,UAAU,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAE;MACtH;MAAA,CACD;IACH;;IAEA;IACAZ,SAAS,CAAC,MAAM;MACdI,WAAW,CAACD,YAAY,CAACG,KAAK,CAAC;IACjC,CAAC,CAAC;IAEF,SAASO,SAASA,CAACC,SAAS,EAAE;MAC5B,MAAMC,OAAM,GAAI,IAAIC,IAAI,CAACb,YAAY,CAACG,KAAK,CAAC;MAC5CS,OAAO,CAACE,OAAO,CAACF,O